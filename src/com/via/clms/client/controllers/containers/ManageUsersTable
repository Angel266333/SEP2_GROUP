package com.via.clms.client.controllers.containers;

import com.via.clms.shared.User;
import javafx.scene.control.Label;

public class ManageUsersTable extends ClickableTable<User> {
	@Override
	Label[] makeLabels(User dataElement) {
		Label[] labels = new Label[3];
		labels[0] = new Label("" + dataElement.uid);
		labels[1] = new Label("" + dataElement.cpr);
		labels[2] = new Label(dataElement.name);
		return labels;
	}

	@Override
	Label[] makeHeaderLabels() {
		Label[] labels = new Label[3];
		labels[0] = new Label("ID");
		labels[1] = new Label("CPR");
		labels[2] = new Label("Name");
		return labels;
	}
}
